	issn = "1063-6919",
	journal = "Computer Vision and Pattern Recognition, 2001. CVPR 2001. Proceedings of the 2001 IEEE Computer Society Conference on",
	keywords = "(artificial AdaBoost; applications; background boosted cascade; classification; classifiers; detection detection; extraction; face feature focus-of-attention guarantees; image image; integral intelligence); learning learning; machine mechanism; object processing; rapid real-time regions; representation; simple specific statistical visual",
	pages = "I--511--I--518 vol.1",
	timestamp = "2008.04.01",
	title = "{Rapid object detection using a boosted cascade of simple features}",
	url = "http://www.bibsonomy.org/bibtex/2a740c4bc2c49dbe70127b9680d2002aa/mozaher",
